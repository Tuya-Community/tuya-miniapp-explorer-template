<view class="page" data-plus-theme="{{theme}}">
  <view class="page__hd">
    <view class="page__title">Gallery</view>
    <view class="page__desc">{{i18n.t('gallery_title')}}</view>
  </view>
  <mpgallery
    show="{{true}}"
    bindchange="change"
    binddelete="delete"
    bindhide="hide"
    img-urls="{{imgUrls}}"
    hide-on-click="{{true}}"
    current="{{1}}"
    show-delete="{{false}}"
  ></mpgallery>
</view>
