<view class="page" data-tyui-theme="{{theme}}">
    <view class="page__hd">
        <view class="page__title">Gallery</view>
        <view class="page__desc">画廊，类似原生的wx.previewImage的展示。</view>
    </view>
    <mpgallery show="{{true}}" bindchange="change" binddelete="delete" bindhide="hide" img-urls="{{imgUrls}}" hide-on-click="{{true}}" current="{{1}}" show-delete="{{false}}"></mpgallery>
</view>
