<view class="container">
  <view class="page-head">
    <view class="page-head-title">addPhoneContact</view>
    <view class="page-head-line"></view>
  </view>
  <view class="page-body">
    <form bindsubmit="submit">
      <view class="page-section">
        <view class="plus-cells__title">姓氏</view>
        <view class="plus-cells plus-cells_after-title">
          <view class="plus-cell plus-cell_input">
            <input class="plus-input" name="lastName" value="{{lastName}}" />
          </view>
        </view>
      </view>
      <view class="page-section">
        <view class="plus-cells__title">名字</view>
        <view class="plus-cells plus-cells_after-title">
          <view class="plus-cell plus-cell_input">
            <input class="plus-input" name="firstName" value="{{firstName}}" />
          </view>
        </view>
      </view>
      <view class="page-section">
        <view class="plus-cells__title">手机号</view>
        <view class="plus-cells plus-cells_after-title">
          <view class="plus-cell plus-cell_input">
            <input class="plus-input" name="mobilePhoneNumber" value="{{mobilePhoneNumber}}" />
          </view>
        </view>
      </view>
      <view class="btn-area">
        <button type="primary" formType="submit">创建联系人</button>
        <button type="default" formType="reset">重置</button>
      </view>
    </form>
  </view>
</view>
