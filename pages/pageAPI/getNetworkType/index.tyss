.page-body-info {
  height: 100px;
}
.page-body-text-network-type {
  font-size: 40px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page-head {
  padding: 30px 25px 40px;
  text-align: center;
}
.page-head-title {
  display: inline-block;
  padding: 0 20px 10px 20px;
  font-size: 16px;
}
.page-head-line {
  margin: 0 auto;
  width: 75px;
  height: 1px;
  background-color: rgba(0, 0, 0, 0.5);
}

.page-body-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 100%;
  padding: 25px 0 75px 0;
}

.page-body-title {
  margin-bottom: 50px;
  font-size: 16px;
}

.page-body-text {
  font-size: 15px;
  line-height: 26px;
  color: rgba(0, 0, 0, 0.3);
}

.btn-area {
  margin-top: 30px;
  box-sizing: border-box;
  width: 100%;
  padding: 0 15px;
}

