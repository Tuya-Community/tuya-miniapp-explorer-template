<view class="container">
  <view class="page-head">
    <view class="page-head-title">makePhoneCall</view>
    <view class="page-head-line"></view>
  </view>
  <view class="page-body">
    <view class="page-section">
      <view class="desc">Enter phone number</view>
      <input class="input" type="number" name="input" bind:input="bindInput" />
      <view class="btn-area">
        <button type="primary" bind:tap="makePhoneCall">call</button>
      </view>
    </view>
  </view>
</view>
